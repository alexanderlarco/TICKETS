        <div id="Titulo">Agregar Impuesto Renta</div>
        <div id="NombreImpuestoRenta">
            <label>Nombre Impuesto Renta:</label>
            <input type="text" name="nombreImpuestoRenta" id="nombreImpuestoRenta" required>
        </div>
        <div id="CodigoImpuestoRenta">
            <label>Codigo Impuesto Renta</label>
            <input type="text" name="codigoImpuestoRenta" id="codigoImpuestoRenta" required>
