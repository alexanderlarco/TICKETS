<LINK REL=StyleSheet HREF="/css/login/registro.css" TYPE="text/css" MEDIA=screen>
<form action="/Registro" method="POST" enctype="multipart/form-data">
  <div id="contenedor">
    <div id="titulo">
      <p>Registra tu Usuario y Contrase単a</p>
    </div>
    <div id="idUsuarios">
      {{#each usuario}}
