      {{/each}}
    </div>
    <div id="imagenUsuario">
      <input type="file" name="imagenUsuario" id="usuario" required>
    </div>
    <div id="datos">
      <input type="text" id="Nombre" name="username" placeholder="Usuario:" required>
    </div>
    <div id="contrase単as">
      <input type="password" id="Contrase単a" name="password" placeholder="Contrase単a:" required>
    </div>
    <div id="guardar">
