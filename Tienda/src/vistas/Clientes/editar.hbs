<LINK REL=StyleSheet HREF="/css/clientes/clientesEditar.css" TYPE="text/css" MEDIA=screen>
<div id="contenedor">
    {{#each clientes}}
    <form action="/clientes/editar/{{idClientes}}" method="POST" id="f1">
        <p>Editar Clientes</p>
        <div id="CEDULA">
            <label id="">Cédula:</label>
            <input type="text" name="username" id="cedula" value="{{username}}">
        </div>
        <div id="NOMBRES">
            <label id="codigos">Nombres:</label>
            <input type="text" name="Nombres" value="{{Nombres}}">
        </div>
        <div id="DIRECCION">
            <label id="producto">Dirección :</label>
            <input type="text" name="Direccion" value="{{Direccion}}">
        </div>
        <div id="TELEFONO">
            <label id="cantidad">Teléfono:</label>
            <input type="text" name="telefono"  value="{{telefono}}">
        </div>
        <div id="CELULAR">
            <label id="precios">Celular:</label>
            <input type="text" name="Celular"  value="0{{Celular}}">
        </div>
        <div id="boton">
            <button type="submit" id="guardar1">Guardar</button>
        </div>
    </form>
    {{/each}}
</div>