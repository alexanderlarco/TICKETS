<form action="/clientes/lista/{{user.idDueñoTienda}}"></form>
<LINK REL=StyleSheet HREF="/css/clientes/clientesLista.css" TYPE="text/css" MEDIA=screen>
<div id="contenedor">
    <div class="row">
        {{#each clientes}}
        <div id="tarjeta">
            <div id="CODIGO">
                <label>Cédula:</label>
                <input type="text" value="{{usernameCliente}}" id="codigo" readonly></input>
            </div>
            <div id="NOMBRES">
                <label>Nombres:</label>
                <input type="text" value="{{NombresCliente}}" id="nombres" readonly></input>
            </div>
            <div id="DIRECCION">
                <label>Dirección:</label>
                <input type="text" value="{{DireccionCliente}}" id="Direccion" readonly></input>
            </div>
            <div id="CELULAR">
                <label>Celular:</label>
                <input type="text" value="{{CelularCliente}}" id="celular" readonly></input>
            </div>
            <div id="Editar">
                <a href="/clientes/editar/{{idClientes}}">
                    <img src="/img/iconos/edit.png">
                </a>
            </div>
            <div id="Eliminar">
                <a href="/clientes/Eliminar/{{idClientes}}">
                    <img src="/img/iconos/borrar.png"></a>
            </div>
            <div id="Agregar">
                <a href="/clientes/Perfil/{{idClientes}}"><img src="/img/iconos/iconUsuario.png"></a></a>
            </div>
        </div>
        {{else}}
        <div id="masProveedor">
            <a href="/clientes/agregar/{{user.idDueñoTienda}}">
                <div id="boton">
                    <button type="button"><img src="/img/iconos/mas1.png" id="mas"></button>
                </div>
            </a>
        </div>
        {{/each}}
    </div>
    <div id="masProveedor">
        <a href="/clientes/agregar/{{user.idDueñoTienda}}">
            <div id="boton">
                <button type="button"><img src="/img/iconos/mas1.png" id="mas"></button>
            </div>
        </a>
    </div>
</div>