<link rel="stylesheet" href="">
<form action="/tipoAmbiente/lista/{{user.idUsuarios}}" method="get"></form>
<div id="contenedor">
    <div class="row">
        {{#each lista}}
        <div id="tarjeta">
            <div id="IdTipoAmbiente">
                <p>{{idTipoAmbiente}}</p>
            </div>
            <div id="nombreTipoAmbiente">
                <p>{{nombreTipoAmbiente}}</p>
            </div>
            <div id="estadoTipoAbiento">
                <p>{{estadoTipoAmbiente}}</p>
            </div>
            <div id="boton">
                <div id="editar">
                    <a href="/tipoAmbiente/editar/{{idTipoAmbiente}}"><button type="button">Editar</button></a>
                </div>
                <div id="eliminar">
                    <a href="/tipoAmbiente/eliminar/{{idTipoAmbiente}}"><button type="button">Elimianr</button></a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>