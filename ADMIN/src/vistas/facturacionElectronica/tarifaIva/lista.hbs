<link rel="stylesheet" href="/css/facturaElectronica/tarifaIva/lista.css">
<form action="/tarifaIva/lista/{{user.idUsuarios}}" method="get"></form>
<div id="contenedor">
    <div class="row">
        {{#each lista}}
        <div id="tarjeta">
            <div id="idTarifa">
                <p>{{nombreTarifaIva}}</p>
            </div>
            <div id="nombre">
                <p>{{nombreTarifaIva}}</p>
            </div>
            <div id="codigo">
                <p>{{codigoTarifaIva}}</p>
            </div>
            <div id="boton">
                <div id="editar">
                    <a href="/tarifaIva/editar/{{idTarifaIva}}"><button type="button">Editar</button></a>
                </div>
                <div id="eliminar">
                    <a href="/tarifaIva/eliminar/{{idTarifaIva}}"><button type="button">Eliminar</button></a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>