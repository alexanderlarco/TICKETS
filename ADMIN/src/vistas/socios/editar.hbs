<link rel="stylesheet" href="/css/actualizacion.css">
{{#each socios}}
<form action="/Socio/editar/{{id}}" method="post">
<div id="contenedor">
 
<div id="ID">
    <p>{{id}}</p>
</div>
<div id="NOMBRES">
    <label id="name">Nombres:</label>
    <input type="text" name="name" id="nombre" value="{{name}}" readonly>
</div>
<div id="usuario">
    <label id="ci">Cédula:</label>
    <input type="text" name="dni" id="cedula" maxlength="10" value="{{dni}}" checked>
</div>
<div id="estado">
    <label id="estadoSocio">estado</label>
    <input type="text" name="status" id="contraseñas" checked value="{{status}}">
</div>
<div id="boton">
<a href="/Socio/Actualizacion/{{id}}"><button type="button" id="guardar">editar</button></a>
</div>
{{/each}}
</form>
</div>